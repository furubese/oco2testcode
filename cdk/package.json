{
  "name": "co2-analysis-cdk",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for CO2 Anomaly Analysis System",
  "bin": {
    "co2-analysis-cdk": "bin/co2-analysis-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "cdk deploy --all",
    "cdk:deploy:base": "cdk deploy BaseStack",
    "cdk:deploy:network": "cdk deploy NetworkStack",
    "cdk:deploy:storage": "cdk deploy StorageStack",
    "cdk:deploy:compute": "cdk deploy ComputeStack",
    "cdk:deploy:frontend": "cdk deploy FrontendStack",
    "cdk:deploy:monitoring": "cdk deploy MonitoringStack",
    "cdk:destroy": "cdk destroy --all",
    "cdk:synth": "cdk synth",
    "cdk:bootstrap": "cdk bootstrap",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.ts'"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "aws-cdk": "2.1031.0",
    "cdk-nag": "^2.37.55",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.221.1",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
